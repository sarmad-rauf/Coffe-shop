			 <form class="form-signin" method="post" action="/">
				<input
				  style="height: 100px;"
				  type="text"
				  name="comment"
				  placeholder="Enter your comment"
				  class="form-control"
				  id="comment"
				/>
				<input
				  style="height: 100px;"
				  type="text"
				  name="comment_name"
				  placeholder="Enter your name"
				  class="form-control"
				  id="comment_name"
				/>
				<button class="btn btn-dark mt-3" type="submit">
				  Submit
				</button>
				<br>
				<br>
			  </form>
			  <main>
				<div class="row">
				  <div class="col-sm-12">

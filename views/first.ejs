							<thead>
								<tr>
									<th>Name</th>
									<th>Comment</th>
									
									<th colspan="2">action</th>
								</tr>
							</thead>
							<tbody>
								<% for (var i = 0; i < comments.length ; i++) { %>
								<tr>
									<td> <%= comments[i].comment_name  %></td>
									<td><%= comments[i].comment  %></td>
									<td><a href="#">edit</a></td>
									<td><a href="#">delete</a></td>
									<td><button>reply</button></td>
								</tr>
								<% } %>
							</tbody>	
						</table>
					</div>
				  </div>
